<Window x:Class="projet_c.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:projet_c"
        mc:Ignorable="d"
        Title="Ways" Height="500" Width="888" WindowStartupLocation="CenterScreen">
    <Grid Margin="0,0,3,6">
        <TextBox HorizontalAlignment="Center" Text="" TextWrapping="Wrap" VerticalAlignment="Center" Width="174" TextChanged="TextBox_TextChanged_1" Height="26" TextAlignment="Center" FontSize="16" Background="#FF6B6BD1" x:Name='textSurname'/>
        <Button Content="Valider" HorizontalAlignment="Center" Margin="0,338,0,0" VerticalAlignment="Top" Width="97" Height="29" Click="buttonValidationSurname_Click"/>
        <Button Content="Admin" HorizontalAlignment="Left" Margin="802,28,0,0" VerticalAlignment="Top" Click="buttonAdmin_Click" Height="28" Width="54"/>
        <TextBox HorizontalAlignment="Center" Margin="0,289,0,0" Text="" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" x:Name='textbox_surname' TextChanged="textbox_surname_TextChanged"/>
        <Label Content="Label:" HorizontalAlignment="Left" Margin="286,0,0,0" VerticalAlignment="Center" BorderBrush="#FF4D4D4D" FontSize="18" Foreground="Black" Background="{x:Null}"/>
    </Grid>
</Window>